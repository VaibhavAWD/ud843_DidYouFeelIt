<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Name of the app [CHAR LIMIT=20] -->
    <string name="app_name">Did You Feel It?</string>

    <!-- Label for perceived strength value [CHAR LIMIT=40] -->
    <string name="perceived_strength">Perceived Strength</string>

    <!-- Number of people who reported perceived strength for an earthquake [CHAR LIMIT=NONE] -->
    <string name="num_people_felt_it"><xliff:g id="number" example="10">%s</xliff:g> people felt it</string>

    <!-- Menu option Settings -->
    <string name="action_settings">Settings</string>

    <!-- Settings Activity Title -->
    <string name="activity_label_earthquake_settings">Earthquake Settings</string>

    <!-- Minimum Magnitude -->
    <string name="pref_label_min_mag">Minimum Magnitude</string>
    <string name="pref_key_min_mag" translatable="false">magnitude</string>
    <string name="pref_default_min_mag" translatable="false">1.0</string>

    <!-- Minimum People Felt -->
    <string name="pref_label_min_felt">Minimum people felt</string>
    <string name="pref_key_min_felt" translatable="false">felt</string>
    <string name="pref_default_min_felt" translatable="false">50</string>

    <!-- Error Message for No internet connectivity -->
    <string name="error_no_internet_connection">No internet connection.</string>

    <!-- Error Message for No earthquake results -->
    <string name="error_no_results_found">No earthquake results found.</string>

    <!-- Label for Button retry to show user call to action to try loading data -->
    <string name="btn_retry">Retry</string>

</resources>
